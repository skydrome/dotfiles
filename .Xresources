Xft*dpi:                    97
Xft*antialias:              true
Xft*rgba:                   rgb
Xft*hinting:                true
Xft*hintstyle:              hintmedium
Xft*lcdfilter:              lcddefault

URxvt*termName:             rxvt-unicode-256color
URxvt*iconFile: .icons/mistEEk/scalable/apps/24/terminal.svg
URxvt*font:     xft:DejaVu Sans Mono:pixelsize=11
URxvt*boldFont: xft:DejaVu Sans Mono:bold:pixelsize=11
URxvt*perl-ext-common: default,clipboard,matcher,tabbed
URxvt*urlLauncher:  /usr/bin/chromium
URxvt*matcher.button:   1

URxvt*buffered:             true
URxvt*scrollBar:            false
URxvt*scrollBar_right:      false
URxvt*borderLess:           false
URxvt*secondaryWheel:       true
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true
URxvt*saveLines:            5000
URxvt*urgentOnBell:         true

URxvt*depth:                32
URxvt*geometry:             86x26
URxvt*internalBorder:       3
URxvt*lineSpace:            0

! Tabs
URxvt*tabbed.tabbar-fg:     8
URxvt*tabbed.tabbar-bg:     0
URxvt*tabbed.tab-fg:        3
URxvt*tabbed.tab-bg:        0

! Cursor
URxvt*cursorBlink:          true
URxvt*cursorUnderline:      false
URxvt*pointerBlank:         true
URxvt*pointerBlankDelay:    2

! Colors
URxvt*cursorColor:          #66ff66
URxvt*pointerColor:         gray
URxvt*fading:               25
URxvt*fadeColor:            #000000

*foreground: #c8c8c8
*background: [90]#111111

!black
*color0:  #251f1f
*color8:  #5e5e5e
!red
*color1:  #eb4509
*color9:  #eb4509
!green
*color2:  #94e76b
*color10: #95e76b
!yellow
*color3:  #ffac18
*color11: #ffac18
!blue
*color4:  #46aede
*color12: #46aede
!magenta
*color5:  #e32c57
*color13: #e32c57
!cyan
*color6:  #06989a
*color14: #06989a
!white
*color7:  #efefef
*color15: #efefef
