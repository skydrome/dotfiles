Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintmedium
Xft.lcdfilter:              lcddefault


URxvt.iconFile: /usr/share/icons/Tango/scalable/apps/terminal.svg
URxvt.termName: rxvt-unicode-256color
URxvt.font:     xft:DejaVuSansMono:size=11
URxvt.boldFont: xft:DejaVuSansMono:size=11
URxvt.perl-ext-common: default,matcher,tabbedex,clipboard

URxvt.matcher.button:   1
URxvt.urlLauncher:      firefox-nightly
!URxvt.matcher.pattern.1: \\b(mailto|http|https|ftp|file):[/]*[\\w-]\\.[\\w./?&@#-]*[\\w/-]
!URxvt.matcher.pattern.2: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt.colorURL:         yellow

URxvt.buffered:             true
URxvt.scrollBar:            false
URxvt.scrollTtyOutput:      false
URxvt.scrollWithBuffer:     true
URxvt.saveLines:            1000

URxvt.depth:                32
URxvt.geometry:             86x26
URxvt.internalBorder:       3
URxvt.lineSpace:            0

! Tabs
URxvt.tabbed.tabbar-fg:     8
URxvt.tabbed.tabbar-bg:     0
URxvt.tabbed.tab-fg:        3
URxvt.tabbed.tab-bg:        0

! Cursor
URxvt.cursorColor:#888888
URxvt.cursorBlink:          true
URxvt.cursorUnderline:      false
URxvt.secondaryScroll:      true
URxvt.pointerColor:         gray
URxvt.pointerColorBackground:black
URxvt.pointerBlank:         false
URxvt.pointerBlankDelay:    2

! Colours
*.background:               [90]#050505
*.foreground:               #0AFF0A
URxvt.fading:               50
!URxvt.fadeColor:            #000000
URxvt.dynamicColors:on


*color0:  rgb:00/00/00
*color1:  rgb:4C/8C/38
*color2:  rgb:27/CA/25
*color3:  rgb:A4/00/8A
*color4:  rgb:5f/7b/8a
*color5:  rgb:1D/90/B6
*color6:  rgb:68/68/68
*color7:  rgb:ff/ff/ff
*color8:  rgb:3C/3C/3C
*color9:  rgb:D6/93/00
*color10: rgb:95/c7/49
*color11: rgb:BB/00/5e
*color12: rgb:6b/7b/8a
*color13: rgb:BB/7D/00
*color14: rgb:36/7F/C5
*color15: rgb:ff/ff/ff

