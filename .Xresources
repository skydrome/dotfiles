Xft*dpi:                    96
Xft*antialias:              true
Xft*rgba:                   rgb
Xft*hinting:                true
Xft*hintstyle:              hintfull
Xft*lcdfilter:              lcddefault

URxvt*termName:  rxvt-256color
URxvt*iconFile: .icons/ACYL_Icon_Theme_0.9.4/scalable/apps/terminal.svg
URxvt*font:     xft:DejaVuSansMono:pixelsize=11
URxvt*boldFont: xft:DejaVuSansMono:pixelsize=11:bold

URxvt*perl-ext-common:      default,clipboard,matcher,tabbed
URxvt*url-launcher:         xdg-open
URxvt*matcher.button:       1

URxvt*buffered:             true
URxvt*scrollBar:            false
URxvt*scrollBar_right:      false
URxvt*borderLess:           false
URxvt*secondaryWheel:       true
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true
URxvt*saveLines:            3000
URxvt*urgentOnBell:         true
URxvt*underlinecolor: #999999
URxvt*cutchars: "()*,<>[]{}|'"
URxvt*depth:                32
URxvt*geometry:             94x28
URxvt*internalBorder:       4
URxvt*lineSpace:            0

! Tabs
URxvt*tabbed.tabbar-fg:     8
URxvt*tabbed.tabbar-bg:     0
URxvt*tabbed.tab-fg:        3
URxvt*tabbed.tab-bg:        0

! Cursor
URxvt*cursorBlink:          true
URxvt*cursorUnderline:      false
URxvt*pointerBlank:         true
URxvt*pointerBlankDelay:    2

! Colors
URxvt*cursorColor:          #66ff66
URxvt*pointerColor:         gray
URxvt*fading:               25
URxvt*fadeColor:            #000000

*foreground: #d0d0d0
URxvt*background: [75]#0f0f0f

! Black / Grey
*color0:       #000000
*color8:       #555753

! Red / Bright Red
*color1:       #cc0000
*color9:       #FF3334

! Green + Bright Green
*color2:       #67cc08
*color10:      #8ae234

! Yellow (Orange) + Bright Yellow (Yellow)
*color3:       #f99157
*color11:      #fce94f

! Blue + Bright Blue
*color4:       #3465a4
*color12:      #6699cc

! Magenta (Purple) + Bright Magenta
*color5:       #77507b
*color13:      #b777e0

! Cyan (Aqua) + Bright Cyan
*color6:       #06989a
*color14:      #34E2E2

! Light Grey (Selection) + White (Current Line)
*color7:       #ABABAB
*color15:      #D3D7CF
