Xft*dpi:                    97
Xft*antialias:              true
Xft*rgba:                   rgb
Xft*hinting:                true
Xft*hintstyle:              hintfull
Xft*lcdfilter:              lcddefault

URxvt*iconFile: .icons/mistEEk/scalable/apps/24/terminal.svg
URxvt.font:     xft:DejaVuSansMono:pixelsize=11,xft:Symbola:pixelsize=10
URxvt.boldFont: xft:DejaVuSansMono:pixelsize=11:bold,xft:Symbola:pixelsize=6:bold
URxvt*perl-ext-common:      default,clipboard,matcher,tabbed
URxvt*urlLauncher:          chromium
URxvt*matcher.button:       1

URxvt*buffered:             true
URxvt*scrollBar:            false
URxvt*scrollBar_right:      false
URxvt*borderLess:           false
URxvt*secondaryWheel:       true
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true
URxvt*saveLines:            5000
URxvt*urgentOnBell:         true
URxvt.underlinecolor: #999999
URxvt.cutchars: "()*,<>[]{}|'"
URxvt*depth:                32
URxvt*geometry:             90x30
URxvt*internalBorder:       0
URxvt*lineSpace:            0

! Tabs
URxvt*tabbed.tabbar-fg:     8
URxvt*tabbed.tabbar-bg:     0
URxvt*tabbed.tab-fg:        3
URxvt*tabbed.tab-bg:        0

! Cursor
URxvt*cursorBlink:          true
URxvt*cursorUnderline:      false
URxvt*pointerBlank:         true
URxvt*pointerBlankDelay:    2

! Colors
URxvt*cursorColor:          #66ff66
URxvt*pointerColor:         gray
URxvt*fading:               25
URxvt*fadeColor:            #000000

*foreground: #c8c8c8
*background: [90]#111111

!black
*color0:  #251f1f
*color8:  #5e5e5e
!red
*color1:  #ff8d8d
*color9:  #eb4509
!green
*color2:  #93d44f
*color10: #95e76b
!yellow
*color3:  #eab93d
*color11: #ffac18
!blue
*color4:  #3465a4
*color12: #46aede
!magenta
*color5:  #f57900
*color13: #e32c57
!cyan
*color6:  #46a4ff
*color14: #06989a
!white
*color7:  #cccccc
*color15: #efefef
